{
  "compilerOptions": {
    "composite": true,
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es5",
      "es2015.promise",
      "dom"
    ],
    "noEmit": true,
    "strict": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "sourceMap": true,
    "stripInternal": true
  }
}
